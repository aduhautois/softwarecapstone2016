        <TextBox HorizontalAlignment="Left" Height="23" Margin="155,155,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" x:Name="email"/>
        <TextBox HorizontalAlignment="Left" Height="24" Margin="155,188,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" x:Name="regionId"/>
        <TextBox HorizontalAlignment="Left" Height="23" Margin="155,222,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" x:Name="zip"/>
        <Button Content="Save" HorizontalAlignment="Left" Margin="190,370,0,-120.4" VerticalAlignment="Top" Width="75" IsEnabled="False" Name="save"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="28,370,0,-120.4" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
